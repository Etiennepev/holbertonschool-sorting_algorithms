#include "sort.h"
/**
 * @insertion_sort_list - sorts a doubly linked list of integers in ascending order using the Insertion sort algorithm
 * @list: pointer to the node
 */

void insertion_sort_list(listint_t **list)
{
	listint_t *current = (*list)->next;

	if (list == NULL || < 2)
		return;
	while (current)
	{

	listint_t *next_node = current->next;
	listint_t *prev_node = current->prev;
	
	while (prev->node && current->n < prev->node->n)
	{
	if (current->next)
	current->next->prev = current->prev;
	if (current->prev)
	current->prev->next = current->next;

	current->prev = node->prev->prev;
	current->next = node->prev;
	if (node->prev->prev->next = current;
	else
	*list = current;
	node->prev->prev = current;
	print_list(*list);
	node_prev = current->prev;
	}
	current = node->next;
}
